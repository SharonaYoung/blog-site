---
layout: 'base.njk'
---

<article class="project">
  <div class="container flow">
    <h1 class="-title"> {{ title }} </h1>
    <p class="tech-tags">{{ tech }}</p>
    <figure class="project__figure">
      <img src="{{ image }}" alt="{{ imageAlt }}">
      <figcaption> {{ imageAlt }} </figcaption>
    </figure>
  {{ content | safe }}
  </div>
</article>